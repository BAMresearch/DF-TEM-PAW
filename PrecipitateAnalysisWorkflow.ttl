@prefix base: <https://github.com/BAMresearch/DF-TEM-PAW/raw/main/PrecipitateAnalysisWorkflow.ttl/> .
@prefix co: <https://w3id.org/pmd/co/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ns1: <https://material-digital.de/pmdco/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

base: a owl:Ontology ;
    dc:contributer "Thomas Hanke, Fraunhofer IWM\"@de" ;
    dc:creator "Bernd Bayerlein (BAM)" ;
    dc:title "Precipitate Analysis Workflow Ontology (PAWO)" ;
    owl:versionInfo "1.0.1" .

base:diskRadius_ a owl:NamedIndividual,
        co:Metadata,
        co:Radius,
        co:SetPoint ;
    co:characteristic base:medianFilter_ .

base:kernel_ a owl:NamedIndividual,
        co:Metadata,
        co:SetPoint,
        co:ValueObject ;
    co:characteristic base:dilation_ .

base:thresholdMethod_ a owl:NamedIndividual,
        co:Metadata,
        co:Method,
        co:SetPoint ;
    co:characteristic base:thresholdImage_ .

ns1:hasCharacteristic a owl:ObjectProperty .

co:characteristic a owl:ObjectProperty .

co:input a owl:ObjectProperty .

co:nextProcess a owl:ObjectProperty .

co:output a owl:ObjectProperty .

co:subordinateProcess a owl:ObjectProperty .

base:AcceleratedCreep a owl:NamedIndividual,
        co:TransmissionElectronMicroscopy ;
    ns1:hasCharacteristic base:specimenAgingTemperature_,
        base:specimenAgingTime_,
        base:specimenCreepStess_ ;
    co:nextProcess base:darkfieldTransmissionElectronMicroscopyImaging_ ;
    co:output base:transmissionElectronMicroscopeSpecimen_ .

base:DFTEMImaging a owl:NamedIndividual ;
    co:output base:darkfieldTransmissionElectronMicroscopeImage_ .

base:ExecutionDate a owl:NamedIndividual,
        co:CreepStress .

base:InvestigationPosition a owl:NamedIndividual,
        co:CreepStress .

base:darkfieldTransmissionElectronMicroscopeImage_ a owl:NamedIndividual,
        co:Image ;
    co:input base:precipitateAnalysisWorkflow_ .

base:darkfieldTransmissionElectronMicroscopyImaging_ a owl:NamedIndividual,
        co:TransmissionElectronMicroscopy ;
    ns1:hasCharacteristic base:InvestigationPosition ;
    co:nextProcess base:precipitateAnalysisWorkflow_ .

base:precipitateRegion_ a owl:NamedIndividual,
        co:Dataset,
        co:Measurement .

base:renderImage_ a owl:NamedIndividual,
        co:AnalysingProcess ;
    co:nextProcess base:grayscaleConversion_ .

base:specimenAgingTemperature_ a owl:NamedIndividual,
        co:AgingTemperature,
        co:Metadata .

base:specimenAgingTime_ a owl:NamedIndividual,
        co:AgingTime,
        co:Metadata .

base:specimenCreepStess_ a owl:NamedIndividual,
        co:CreepStress,
        co:Metadata .

base:specimenName_ a owl:NamedIndividual,
        co:Metadata,
        co:SpecimenName .

base:transmissionElectronMicroscopeSpecimen_ a owl:NamedIndividual,
        co:Specimen ;
    ns1:hasCharacteristic base:specimenName_ ;
    co:input base:AcceleratedCreep,
        base:DFTEMImaging .

co:AgingTemperature a owl:Class .

co:AgingTime a owl:Class .

co:Analysingprocess a owl:Class .

co:Dataset a owl:Class .

co:Image a owl:Class .

co:Measurement a owl:Class .

co:MeasuringProcess a owl:Class .

co:Method a owl:Class .

co:Radius a owl:Class .

co:Specimen a owl:Class .

co:SpecimenName a owl:Class .

co:ValueObject a owl:Class .

base:clearBorder_ a owl:NamedIndividual,
        co:AnalysingProcess ;
    co:nextProcess base:findContour_ .

base:findContour_ a owl:NamedIndividual,
        co:MeasuringProcess ;
    co:nextProcess base:labelPrecipitates_ .

base:grayscaleConversion_ a owl:NamedIndividual,
        co:AnalysingProcess ;
    co:nextProcess base:medianFilter_ .

base:labelPrecipitates_ a owl:NamedIndividual,
        co:Analysingprocess .

base:morphologicalOpening_ a owl:NamedIndividual,
        co:AnalysingProcess ;
    co:nextProcess base:clearBorder_ .

base:precipitateAnalysisWorkflow_ a owl:NamedIndividual,
        co:AnalysingProcess ;
    ns1:hasCharacteristic base:ExecutionDate ;
    co:output base:precipitateRegion_ ;
    co:subordinateProcess base:clearBorder_,
        base:dilation_,
        base:findContour_,
        base:grayscaleConversion_,
        base:labelPrecipitates_,
        base:medianFilter_,
        base:morphologicalOpening_,
        base:renderImage_,
        base:thresholdImage_ .

co:TransmissionElectronMicroscopy a owl:Class .

base:dilation_ a owl:NamedIndividual,
        co:AnalysingProcess ;
    co:nextProcess base:morphologicalOpening_ .

base:medianFilter_ a owl:NamedIndividual,
        co:AnalysingProcess ;
    co:nextProcess base:thresholdImage_ .

base:thresholdImage_ a owl:NamedIndividual,
        co:AnalysingProcess ;
    co:nextProcess base:dilation_ .

co:CreepStress a owl:Class .

co:SetPoint a owl:Class .

co:Metadata a owl:Class .

co:AnalysingProcess a owl:Class .
